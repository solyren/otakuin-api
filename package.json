    "sync:map": "bun run src/sync_map.ts"
  },
  "dependencies": {
    "@upstash/redis": "^1.35.3",
    "axios": "^1.7.2",
    "axios-cookiejar-support": "^5.0.2",
    "cheerio": "^1.1.2",
    "dotenv": "^17.2.2",
    "elysia": "^1.3.21",
    "fuse.js": "^7.1.0",
    "tough-cookie": "^4.1.4"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "nodemon": "^3.1.10"
  }
}
