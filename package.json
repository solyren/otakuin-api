{
  "name": "otakuin",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "peerDependencies": {
    "typescript": "^5"
  },
  "scripts": {
    "start": "NODE_TLS_REJECT_UNAUTHORIZED=0 bun run src/index.ts",
    "dev": "nodemon --exec 'NODE_TLS_REJECT_UNAUTHORIZED=0 bun run src/index.ts'",
